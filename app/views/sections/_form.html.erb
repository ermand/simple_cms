<table summary="Subejct form fields">
  <tr>
    <th>Page</th>
    <th><%= f.select :page_id, @pages.collect {|e| [ e.name, e.id ] } %></th>
  </tr>
  <tr>
    <th>Name</th>
    <th><%= f.text_field(:name) %></th>
  </tr>
  <tr>
    <th>Position</th>
    <th><%= f.text_field(:position) %></th>
  </tr>
  <tr>
    <th>Visible</th>
    <th><%= f.text_field(:visible) %></th>
  </tr>
  <tr>
    <th>Content Type</th>
    <th><%= f.text_field(:content_type) %></th>
  </tr>
  <tr>
    <th>Content</th>
    <th><%= f.text_area(:content, size: "30x4") %></th>
  </tr>
</table>
